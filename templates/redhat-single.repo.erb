#
# Certificate-Based Repository
# Managed by rhsm puppet module
#
#
[<%= id %>]
name = <%= name %>
baseurl = <%= baseurl %>
enabled = <%= enabled %>
gpgcheck = <%= gpgcheck %>
gpgkey = <%= gpgkey %>
sslverify = <%= sslverify %>
sslcacert = <%= sslcacert %>
sslclientkey = <%= sslclientkey %>
sslclientcert = <%= sslclientcert %>
metadata_expire = <%= metadata_expire %>